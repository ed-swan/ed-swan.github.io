  title =       {An Empirical User-Based Study of Text Drawing Styles and 
                 Outdoor Background Textures for Augmented Reality}, 
  booktitle =   {Technical Papers, Proceedings of IEEE Virtual Reality 2005}, 
  year =        2005, 
  location =    {Bonn, Germany}, 
